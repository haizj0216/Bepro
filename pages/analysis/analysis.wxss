/* pages/analysis/analysis.wxss */

.title {
  margin-top: 20rpx;
  font-size: 30rpx;
  text-align: center;
}

.head_layout{
display: flex;
  flex-direction: row;
  justify-content: center;
  flex-wrap: wrap;
  margin-top: 30rpx;
}
.head {
  width: 100rpx;
  height: 100rpx;
  overflow:hidden;  
  border-radius: 50%;
  
}

.item-body-items {
  display: flex;
  margin-top: 30rpx;
  margin-left: 50rpx;
  margin-right: 50rpx;
}

.item-body-item {
  display: flex;
  width: 50%;
  align-items: center;
}

.item-body-item-image {
  width: 40rpx;
  height: 40rpx;
}

.item-body-item-text {
  color: rgb(51, 51, 51);
  font-size: 30rpx;
  text-align: center;
}

