<!--pages/test_photo/test_photo.wxml-->
<view>
  <view class="title">
    <text>2/上传照片</text>
  </view>
  <view class="title">请将面部对准手机摄像头或</view>
  <view class="desc1">上传清晰面部清晰照片</view>
  <view class="desc2">仅作为皮肤分析使用</view>
  <view class="image">
    <van-uploader name="1" file-list="{{ fileList1 }}" bind:after-read="afterRead" bind:delete="delete" max-count="{{ 1 }}" bind:click-preview="clickPreview" />
  </view>
  <view class="skinanalyze" bindtap="skinanalyze">开始检测</view>
  <view class="next" bindtap="toTest2">提交更新></view>
</view>